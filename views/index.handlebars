<h2>Add a Cat</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Add Burger Name:</label>
    <input type="text" id="ca" name="name">
    <button type="submit">Add Burger</button>
  </div>
</form>
<div class="col-md-3" >
  {{#each burger_data}}
    {{#unless this.devoured}}
      <div class="col-md-9">
        {{this.id}} . {{this.burger_name}}
        <button class="devour-from">devour</button>
      </div>
    {{/unless}}
  {{/each}}

</div>
